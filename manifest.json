{
  "name": "titleTimer",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "titleTimer",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
	  "http://time-time.net/"
	],
 
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}
